{
  "name": "iot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:1-generate": "prisma generate",
    "build:2-next": "cross-env NODE_ENV=production next build",
    "build:3-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "cross-env PORT=3000 node start-dev.js",
    "dev": "run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production TS_NODE_BASEURL=./dist TS_NODE_PROJECT=./tsconfig.server.json node -r tsconfig-paths/register ./dist/src/server/index.js",
    "studio": "prisma studio",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "test": "jest"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.5",
    "@prisma/client": "^3.0.1",
    "@trpc/client": "^9.23.6",
