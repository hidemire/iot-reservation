    "@trpc/react": "^9.23.6",
    "@trpc/server": "^9.23.6",
    "@types/ioredis": "^4.28.10",
    "@types/ping": "^0.4.1",
    "@xstate/react": "^3.0.0",
    "awilix": "^7.0.3",
    "bullmq": "^3.15.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "dotenv-flow": "^3.2.0",
    "envalid": "^7.3.1",
    "i": "^0.3.7",
    "ion-sdk-js": "^1.8.2",
    "ioredis": "^5.0.5",
    "next": "^12.1.6",
    "next-auth": "^4.2.0",
    "next-themes": "^0.2.0",
    "node-fetch": "^2.6.1",
    "npm": "^8.11.0",
    "ping": "^0.4.2",
    "react": "^18.1.0",
    "react-day-picker": "^8.0.5",
    "react-dom": "^18.1.0",
    "react-joyride": "^2.4.0",
    "react-query": "^3.37.0",
    "start-server-and-test": "^1.12.0",
    "superjson": "^1.7.4",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^4.0.0",
    "ws": "^8.0.0",
    "xstate": "^4.32.1",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.10",
    "@types/node-fetch": "^2.5.11",
    "@types/nodemon": "^1.19.1",
    "@types/react": "^18.0.9",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.1.0",
    "jest-playwright": "^0.0.1",
    "jest-playwright-preset": "^1.4.5",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.19.1",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
